{
  // OpenClaw Config: WhatsApp Only
  // Replace YOUR_* values with real credentials
  
  "models": {
    "providers": {
      "anthropic": {
        "apiKey": "YOUR_ANTHROPIC_API_KEY"
      }
    }
  },

  "agents": {
    "defaults": {
      "model": {
        "primary": "anthropic/claude-sonnet-4-20250514"
      },
      "workspace": "~/.openclaw/workspace"
    }
  },

  "channels": {
    "whatsapp": {
      "enabled": true,
      "allowFrom": ["+YOUR_PHONE_NUMBER"]
    }
  }
}
